    @await RenderSectionAsync("Styles", required: false)
    <style>
        /* Header temel stili */
        #kt_app_header {
            background-color: #1e1e2d;
            border-bottom: 1px solid #2B2B40;
        }

        /* Light modda header arkaplan rengi sabit kalacak */
        [data-bs-theme="light"] #kt_app_header {
            background-color: #1e1e2d !important;
        }

        /* Header menü yazı renkleri */
        .app-header .menu-title {
            color: #ffffff !important;
            font-weight: 500 !important;
        }

        /* Header menü ikonları */
        .app-header .menu-icon i {
            color: #ffffff !important;
            opacity: 1 !important;
        }

        /* Aktif menü öğesi */
        .app-header .menu-item.show .menu-link,
        .app-header .menu-item.hover .menu-link {
            background-color: #2B2B40 !important;
        }

        /* Header menü hover efekti */
        .app-header .menu-link:hover {
            background-color: #2B2B40 !important;
        }

        /* Path renkleri için override */
        .app-header .menu-icon i .path1,
        .app-header .menu-icon i .path2,
        .app-header .menu-icon i .path3,
        .app-header .menu-icon i .path4 {
            color: #ffffff !important;
            opacity: 1 !important;
        }

        /* Light/Dark mod geçişlerinde header içindeki menü öğeleri hep beyaz kalacak */
        [data-bs-theme="light"] .app-header .menu-title,
        [data-bs-theme="dark"] .app-header .menu-title {
            color: #ffffff !important;
        }

        [data-bs-theme="light"] .app-header .menu-icon i,
        [data-bs-theme="dark"] .app-header .menu-icon i {
            color: #ffffff !important;
        }

        /* Tema değiştirme menüsü */
        .menu-sub-dropdown {
            background-color: var(--bs-body-bg) !important;
        }

            .menu-sub-dropdown .menu-link {
                color: var(--bs-body-color) !important;
            }

                .menu-sub-dropdown .menu-link:hover {
                    background-color: var(--bs-gray-100) !important;
                }

        /* Tema değiştirme ikonları */
        .app-header .theme-light-show,
        .app-header .theme-dark-show {
            color: #ffffff !important;
        }

        /* Kullanıcı menüsü ikonları */
        .app-header .symbol-label i {
            color: #ffffff !important;
        }

        [data-bs-theme="light"] .menu-sub-dropdown.menu-sub-dropdown-light {
            background-color: #ffffff !important;
        }

        [data-bs-theme="light"] .menu-sub-dropdown .menu-link {
            color: #3F4254 !important;
        }

            [data-bs-theme="light"] .menu-sub-dropdown .menu-link .menu-title {
                color: #3F4254 !important;
            }

            [data-bs-theme="light"] .menu-sub-dropdown .menu-link:hover {
                background-color: #F4F6FA !important;
            }

        /* Dark modda tema menüsü için özel stiller */
        [data-bs-theme="dark"] .menu-sub-dropdown {
            background-color: #1e1e2d !important;
        }

            [data-bs-theme="dark"] .menu-sub-dropdown .menu-link {
                color: #ffffff !important;
            }

                [data-bs-theme="dark"] .menu-sub-dropdown .menu-link .menu-title {
                    color: #ffffff !important;
                }

                [data-bs-theme="dark"] .menu-sub-dropdown .menu-link:hover {
                    background-color: #2B2B40 !important;
                }

        /* Icon renkleri her iki mod için */
        .menu-sub-dropdown .menu-link .menu-icon i {
            color: #3F4254 !important;
        }

        [data-bs-theme="dark"] .menu-sub-dropdown .menu-link .menu-icon i {
            color: #ffffff !important;
        }
    </style>
    <!--end::Global Stylesheets Bundle-->
</head>

